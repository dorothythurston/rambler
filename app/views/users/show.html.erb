<h1><%= @user.email %></h1>

<%if signed_in?%>
  <%if current_user != @user %>
    <%if current_user.following? @user %>
      <%= button_to "Unfollow", unfollow_user_path(@user), method: :delete %>
    <%else%>
      <%= button_to "Follow", follow_user_path(@user) %>
    <%end%>
  <%end%>
<% end %>

<%= render @rambles %>

